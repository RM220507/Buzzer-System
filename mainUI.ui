<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="customtkinter.CTk" id="rootFrame" named="True">
    <property name="appearance_mode">light</property>
    <property name="color_theme">blue</property>
    <property name="height">760</property>
    <property name="minsize">0|0</property>
    <property name="title" translatable="yes">Buzzer System</property>
    <property name="width">1100</property>
    <child>
      <object class="customtkinter.CTkTabview" id="mainTabview" named="True">
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="customtkinter.CTkTabview.Tab" id="questionTab" named="True">
            <property name="label" translatable="yes">Question</property>
            <child>
              <object class="customtkinter.CTkFrame" id="questionSetInfoFrame" named="True">
                <layout manager="pack">
                  <property name="expand">false</property>
                  <property name="fill">x</property>
                  <property name="padx">5</property>
                  <property name="pady">5</property>
                  <property name="side">top</property>
                </layout>
                <containerlayout manager="grid">
                  <property type="col" id="0" name="weight">5</property>
                </containerlayout>
                <child>
                  <object class="customtkinter.CTkLabel" id="questionSetLabel" named="True">
                    <property name="font">{Bahnschrift SemiBold} 24 {}</property>
                    <property name="text" translatable="yes">No Question Set Selected</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">0</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkLabel" id="roundDescriptor" named="True">
                    <property name="font">{Bahnschrift SemiBold} 20 {}</property>
                    <property name="text" translatable="yes">No round - 0 of 0</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">1</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkButton" id="questionSetEditButton" named="True">
                    <property name="command" type="command" cbtype="simple">editQuestionSet</property>
                    <property name="state">disabled</property>
                    <property name="text" translatable="yes">Edit Set</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">0</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkButton" id="newQuestionSetButton" named="True">
                    <property name="command" type="command" cbtype="simple">newQuestionSet</property>
                    <property name="text" translatable="yes">New Set</property>
                    <layout manager="grid">
                      <property name="column">2</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">0</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkButton" id="loadQuestionSetButton" named="True">
                    <property name="command" type="command" cbtype="simple">loadQuestionSet</property>
                    <property name="text" translatable="yes">Load Set</property>
                    <layout manager="grid">
                      <property name="column">2</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">1</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="customtkinter.CTkFrame" id="currentQuestionFrame" named="True">
                <layout manager="pack">
                  <property name="fill">x</property>
                  <property name="padx">5</property>
                  <property name="pady">5</property>
                  <property name="side">top</property>
                </layout>
                <containerlayout manager="grid">
                  <property type="col" id="0" name="weight">1</property>
                  <property type="col" id="1" name="weight">1</property>
                </containerlayout>
                <child>
                  <object class="customtkinter.CTkLabel" id="questionNumLabel" named="True">
                    <property name="font">{Bahnschrift SemiBold} 16 {}</property>
                    <property name="text" translatable="yes">Question 0 of 0</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">0</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkLabel" id="questionPointsLabel" named="True">
                    <property name="font">{Bahnschrift SemiBold} 16 {}</property>
                    <property name="text" translatable="yes">Points: 0</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">0</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkLabel" id="currentQuestionLabel" named="True">
                    <property name="font">{Bahnschrift SemiBold} 36 {}</property>
                    <property name="text" translatable="yes">Current Question</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">1</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkLabel" id="currentQuestionAnswerLabel" named="True">
                    <property name="font">{Bahnschrift SemiBold} 20 {}</property>
                    <property name="text" translatable="yes">Current Answer</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">2</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkLabel" id="currentQuestionNotesLabel" named="True">
                    <property name="font">{Bahnschrift SemiBold} 20 {}</property>
                    <property name="text" translatable="yes">Current Notes</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">3</property>
                      <property name="sticky">w</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkLabel" id="currentQuestionAid" named="True">
                    <property name="text" translatable="yes">Question Aid!!</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="padx">5</property>
                      <property name="pady">5</property>
                      <property name="row">1</property>
                      <property name="rowspan">3</property>
                      <property name="sticky">nsew</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="customtkinter.CTkFrame" id="buzzerControlFrame" named="True">
                <layout manager="pack">
                  <property name="fill">x</property>
                  <property name="padx">5</property>
                  <property name="pady">5</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="customtkinter.CTkFrame" id="buzzerControlWaitingFrame" named="True">
                    <layout manager="pack">
                      <property name="fill">both</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="customtkinter.CTkLabel" id="buzzerControlWaitingLabel" named="True">
                        <property name="font">{Bahnschrift SemiBold} 20 {}</property>
                        <property name="text" translatable="yes">Waiting for Buzz...</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="buzzerControlWaitingSkipQButton" named="True">
                        <property name="command" type="command" cbtype="simple">skipQuestion</property>
                        <property name="text" translatable="yes">Skip Question</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="buzzerControlWaitingClose" named="True">
                        <property name="command" type="command" cbtype="simple">buzzerClose</property>
                        <property name="text" translatable="yes">Close Entry</property>
                        <layout manager="pack">
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkFrame" id="buzzerControlClosedFrame" named="True">
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="customtkinter.CTkButton" id="buzzerControlClosedOpenAllButton" named="True">
                        <property name="command" type="command" cbtype="simple">buzzerOpenAll</property>
                        <property name="text" translatable="yes">Open to All</property>
                        <layout manager="pack">
                          <property name="expand">true</property>
                          <property name="fill">both</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkOptionMenu" id="buzzerControlClosedTeamSelect" named="True">
                        <layout manager="pack">
                          <property name="expand">true</property>
                          <property name="fill">x</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="customtkinter.CTkButton" id="buzzerControlClosedOpenTeamButton" named="True">
                        <property name="command" type="command" cbtype="simple">buzzerOpenTeam</property>
                        <property name="text" translatable="yes">Open to Team</property>
                        <layout manager="pack">
                          <property name="expand">true</property>
                          <property name="fill">x</property>
                          <property name="padx">5</property>
                          <property name="pady">5</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="customtkinter.CTkFrame" id="buzzerControlBuzzedFrame" named="True">
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="customtkinter.CTkButton" id="buzzer" named="True">
                        <property name="text" translatable="yes">ctkbutton9</property>
                        <layout manager="pack">
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="customtkinter.CTkTabview.Tab" id="configTab" named="True">
            <property name="label" translatable="yes">Configuration</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
